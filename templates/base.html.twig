<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="{{ asset('images/icon/favicon.ico') }}">
        
        {# CSS JQUERY UI #}
        <link href="{{ asset('jquery/jquery-ui-1.13.3/jquery-ui.min.css') }}" rel="stylesheet">
        <link href="{{ asset('jquery/jquery-ui-1.13.3/jquery-ui.structure.min.css') }}" rel="stylesheet">
        <link href="{{ asset('jquery/jquery-ui-1.13.3/jquery-ui.theme.min.css') }}" rel="stylesheet">
            
        {# JS JQUERY #}
        <script src="{{ asset('jquery/jquery-3.7.1.min.js') }}"></script>
        {# JS JQUERY UI #}
        <script src="{{ asset('jquery/jquery-ui-1.13.3/jquery-ui.min.js') }}"></script>

        {# BOOTSTRAP #}
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

        {# PLANTILLA #}
        <style>
            body{
                background: url({{ asset('images/iesfuentezuelas.jpg') }}) !important;
                background-size: 130em !important;
                background-position: center !important;
                background-repeat: no-repeat !important;
                background-attachment: fixed !important;
            }
        </style>

        {% block stylesheets %}{% endblock %}
        {% block javascripts %}{% endblock %}

        {% block myjs %}{% endblock %}
        {% block mycss %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
